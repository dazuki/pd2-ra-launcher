{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "pd2-ra-launcher",
  "version": "2.0.0",
  "identifier": "pd2-ra-launcher",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Project Diablo 2 Reawakening Launcher",
        "width": 1280,
        "height": 720,
        "resizable": false,
        "center": true,
        "decorations": false,
        "fullscreen": false,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "fs": {}
  }
}
